{
  "name": "bp-eecs",
  "version": "1.0.0",
  "description": "An app for browsing EECS course requirements",
  "main": "build/main.js",
  "scripts": {
    "start": "node build/main.js",
    "prestart": "npm install --only=dev && npm run build",
    "build": "npm-run-all --parallel build:*",
    "build:server": "babel src/ --out-dir build/",
    "build:client": "npm run build --prefix client/",
    "serve": "npm-run-all --parallel serve:*",
    "serve:server": "nodemon --ignore client --exec babel-node src/main.js",
    "serve:client": "npm run start --prefix client/",
    "test": "npm-run-all test:*",
    "test:server": "echo \"Error: no test specified\"",
    "test:client": "npm run test --prefix client"
  },
  "author": "C Hundhausen",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mongoose": "^5.11.3",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.7",
    "nodemon": "^2.0.6"
  }
}
